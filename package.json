{
  "name": "openlayers-docset",
  "version": "1.0.0",
  "description": "OpenLayers docset generator",
  "author": "Rifa'i M. Hanif <hanreev@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanreev/openlayers-docset.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "jsdoc jsdoc/index.md -c jsdoc/conf.json -P openlayers/package.json -d OpenLayers.docset",
    "build-dts": "jsdoc -c jsdoc/conf-dts.json -P openlayers/package.json -d typings",
    "lint-dts": "tsc --pretty",
    "format-dts": "tsfmt -r",
    "build-format-dts": "yarn build-dts && yarn format-dts"
  },
  "dependencies": {
    "jsdoc": "^3.5.5",
    "jsdoc-plugin-typescript": "^1.0.7",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@types/arcgis-rest-api": "^10.4.4",
    "@types/geojson": "^7946.0.7",
    "@types/topojson-specification": "^1.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "typescript-formatter": "^7.2.2"
  }
}
