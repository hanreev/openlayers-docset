{
  "opts": {
    "recurse": true,
    "template": "jsdoc/template-dts"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "openlayers/src/ol"
    ]
  },
  "plugins": [
    "jsdoc/plugins/markdown",
    "jsdoc-plugin-typescript",
    "jsdoc/plugins/inline-options",
    "jsdoc/plugins/inheritdoc",
    "jsdoc/plugins/events",
    "jsdoc/plugins/observable",
    "jsdoc/plugins/api-dts",
    "jsdoc/plugins/exports"
  ],
  "typescript": {
    "moduleRoot": "openlayers/src",
    "declaration": {
      "mode": "multiple"
    }
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": false
    },
    "applicationName": "OpenLayers"
  },
  "jsVersion": 180
}
